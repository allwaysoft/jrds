<ivysettings>
    <resolvers>
        <ibiblio name="central" m2compatible="true" root="https://repo.maven.apache.org/maven2"/>
        <ibiblio name="sonaytpe" m2compatible="true" root="https://oss.sonatype.org/content/repositories/snapshots" checkmodified="true" changingMatcher="regexp"/>
        <url name="ivyrepo" m2compatible="true">
            <ivy pattern="http://jrds.fr/maven2/[organisation]/[module]/ivys/ivy-[revision].xml" />
            <artifact
                pattern="http://jrds.fr/maven2/[organisation]/[module]/[type]s/[artifact]-[revision].[ext]" />
        </url>
        <filesystem name="build">
            <ivy pattern="${buildir}/ivy/[organisation]/[module]/ivys/ivy.xml"/>
            <artifact pattern="${buildir}/ivy/[organisation]/[module]/[type]s/[artifact]-[revision].[ext]"/>
        </filesystem>
        <filesystem name="mavenbuild" m2compatible="true">
            <artifact pattern="${buildir}/maven/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
        </filesystem>
    </resolvers>
    <settings defaultResolver="central" />
    <modules>
        <module organisation="org.dojotoolkit" name="*" resolver="ivyrepo"/>
        <module organisation="fr.jrds.verax" name="*" resolver="ivyrepo"/>
        <module organisation="fr.jrds" name="snmpcodec" resolver="sonaytpe"/>
    </modules>
    <include url="${ivy.default.settings.dir}/ivysettings-public.xml" />
    <include url="${ivy.default.settings.dir}/ivysettings-shared.xml" />
    <include url="${ivy.default.settings.dir}/ivysettings-local.xml" />
    <include url="${ivy.default.settings.dir}/ivysettings-main-chain.xml" />
    <include url="${ivy.default.settings.dir}/ivysettings-default-chain.xml" />
</ivysettings>

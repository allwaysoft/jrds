<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
    <name>icmpsnmp</name>
    <graphName>icmp</graphName>
    <graphTitle>ICMP Info on ${host}</graphTitle>
    <verticalLabel>Paquets/s</verticalLabel>
    <add>
        <name>OutMsgs</name>
        <graphType>line</graphType>
        <legend>ICMP messages sent</legend>
    </add>
    <add>
        <name>InMsgs</name>
        <reversed>true</reversed>
        <graphType>line</graphType>
        <legend>ICMP messages received</legend>
    </add>
    <hosttree>
        <pathelement>HOST</pathelement>
        <pathelement>NETWORK</pathelement>
        <pathstring>ICMP Info</pathstring>
    </hosttree>
    <viewtree>
        <pathelement>NETWORK</pathelement>
        <pathstring>ICMP Info</pathstring>
        <pathelement>HOST</pathelement>
    </viewtree>
</graphdesc>
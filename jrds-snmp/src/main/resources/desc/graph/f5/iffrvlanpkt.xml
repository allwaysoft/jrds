<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
    <name>IfF5Vlanpkt</name>
    <graphName>ifpkts-${index}</graphName>
    <graphTitle>Packets exchanged on interface ${index} on ${host}</graphTitle>
    <verticalLabel>packets/s</verticalLabel>
    <lowerLimit>NaN</lowerLimit>
    <add>
        <legend>Upward graph</legend>
    </add>
    <add>
        <name>sysVlanStatPktsOut</name>
        <graphType>area</graphType>
        <color>blue</color>
        <legend>packets sends/s</legend>
    </add>
    <add>
        <name>sysVlanStatErrorsOut</name>
        <graphType>line</graphType>
        <color>red</color>
        <legend>packets in error send/s</legend>
    </add>
    <add>
        <name>sysVlanStatDropsOut</name>
        <graphType>line</graphType>
        <color>orange</color>
        <legend>Dropped packets send/s</legend>
    </add>
    <add>
        <name>sysVlanStatMcastOut</name>
        <graphType>line</graphType>
        <color>black</color>
        <legend>Multicast packets send/s</legend>
    </add>
    <add>
        <legend>Downward graph</legend>
    </add>
    <add>
        <name>sysVlanStatPktsIn</name>
        <reversed>true</reversed>
        <graphType>area</graphType>
        <color>green</color>
        <legend>packets received/s</legend>
    </add>
    <add>
        <name>sysVlanStatErrorsIn</name>
        <reversed>true</reversed>
        <graphType>line</graphType>
        <color>red</color>
        <legend>packets in error received/s</legend>
    </add>
    <add>
        <name>sysVlanStatDropsIn</name>
        <graphType>line</graphType>
        <color>orange</color>
        <legend>Dropped packets send/s</legend>
    </add>
    <add>
        <name>sysVlanStatMcastIn</name>
        <reversed>true</reversed>
        <graphType>line</graphType>
        <color>red</color>
        <legend>Multicast packets received/s</legend>
    </add>
    <hosttree>
        <pathelement>HOST</pathelement>
        <pathelement>NETWORK</pathelement>
        <pathelement>INTERFACES</pathelement>
        <pathelement>INDEX</pathelement>
        <pathstring>Packets exchanged</pathstring>
    </hosttree>
    <viewtree>
        <pathelement>NETWORK</pathelement>
        <pathelement>INTERFACES</pathelement>
        <pathstring>Packets exchanged</pathstring>
        <pathelement>HOST</pathelement>
        <pathelement>INDEX</pathelement>
    </viewtree>
</graphdesc>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "http://bidule">
<graphdesc>
    <name>udpactivity</name>
    <graphName>udp</graphName>
    <graphTitle>UDP activity on {1}</graphTitle>
    <verticalLabel>packets/s</verticalLabel>
    <add>
        <legend>Send packets</legend>
    </add>
    <add>
        <name>OutDatagrams</name>
        <graphType>line</graphType>
        <color>blue</color>
        <legend>Out Datagrams</legend>
    </add>
    <add>
        <legend>Received packets</legend>
    </add>
    <add>
        <name>InDatagrams</name>
        <reversed>true</reversed>
        <graphType>line</graphType>
        <color>green</color>
        <legend>Received Datagrams</legend>
    </add>
    <add>
        <name>NoPorts</name>
        <reversed>true</reversed>
        <graphType>line</graphType>
        <color>teal</color>
        <legend>Datagrams with no listening port</legend>
    </add>
    <add>
        <name>InErrors</name>
        <reversed>true</reversed>
        <graphType>line</graphType>
        <color>olive</color>
        <legend>Invalid</legend>
    </add>
    <hosttree>
        <pathelement>HOST</pathelement>
        <pathelement>NETWORK</pathelement>
        <pathstring>UDP activity</pathstring>
    </hosttree>
    <viewtree>
        <pathelement>NETWORK</pathelement>
        <pathstring>UDP activity</pathstring>
        <pathelement>HOST</pathelement>
    </viewtree>

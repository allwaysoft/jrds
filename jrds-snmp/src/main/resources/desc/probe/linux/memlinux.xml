    </ds>
    <ds>
        <dsName>memShared</dsName>
        <dsType>gauge</dsType>
        <oid>memShared</oid>
    </ds>
    <ds>
        <dsName>memBuffer</dsName>
        <dsType>gauge</dsType>
        <oid>memBuffer</oid>
    </ds>
    <ds>
        <dsName>memCached</dsName>

<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>CpuRawLinuxHost</name>
    <probeName>cpurawlinux</probeName>
    <probeClass>jrds.probe.snmp.RdsSnmpSimple</probeClass>
    <specific name="requester">simple</specific>
    <specific name="existOid">ssCpuRawInterrupt</specific>
    <specific name="hides">CpuRawTimeSolaris, CpuRawTimeLinux</specific>
    <specific name="nodiscover"/>
    <ds>
        <dsName>ssCpuRawUser</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawUser</oid>
    </ds>
    <ds>
        <dsName>ssCpuRawNice</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawNice</oid>
    </ds>
    <ds>
        <dsName>ssCpuRawSystem</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawSystem</oid>
    </ds>
    <ds>
        <dsName>ssCpuRawIdle</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawIdle</oid>
    </ds>
    <ds>
        <dsName>ssCpuRawWait</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawWait</oid>
    </ds>
    <ds>
        <dsName>ssCpuRawInterrupt</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawInterrupt</oid>
    </ds>
    <ds>
        <dsName>ssCpuRawSoftIRQ</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawSoftIRQ</oid>
    </ds>
    <ds>
        <dsName>ssCpuRawSteal</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawSteal</oid>
        <defaultValue>0</defaultValue>
    </ds>
    <ds>
        <dsName>ssCpuRawGuest</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawGuest</oid>
        <defaultValue>0</defaultValue>
    </ds>
    <ds>
        <dsName>ssCpuRawGuestNice</dsName>
        <dsType>counter</dsType>
        <oid>ssCpuRawGuestNice</oid>
        <defaultValue>0</defaultValue>
    </ds>
    <ds>
        <dsName>ssRawInterrupts</dsName>
        <dsType>counter</dsType>
        <oid>ssRawInterrupts</oid>
    </ds>
    <ds>
        <dsName>ssRawContexts</dsName>
        <dsType>counter</dsType>
        <oid>ssRawContexts</oid>
    </ds>
    <graphs>
        <name>CpuRawTimeLinux2</name>
        <name>CpuRawTimeLinuxHost</name>
        <name>InterruptsCount</name>
    </graphs>
</probedesc>

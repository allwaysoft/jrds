<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>PartitionSpace</name>
    <probeName>fs-${index.cleanpath}</probeName>
    <probeClass>jrds.probe.snmp.PartitionSpace</probeClass>
    <specific name="description">Graph space used and free for a mountpoint</specific>
    <specific name="snmpRequester">RAW</specific>
    <specific name="indexOid">hrStorageDescr</specific>
    <ds>
        <dsName>Total</dsName>
        <dsType>gauge</dsType>
        <oid>hrStorageSize</oid>
    </ds>
    <ds>
        <dsName>Used</dsName>
        <dsType>gauge</dsType>
        <oid>hrStorageUsed</oid>
    </ds>
    <ds>
        <oid>hrStorageAllocationUnits</oid>
    </ds>
    <graphs>
        <name>PartitionSpaceGraph</name>
    </graphs>
</probedesc>

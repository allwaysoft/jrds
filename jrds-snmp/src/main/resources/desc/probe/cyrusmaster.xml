<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>CyrusMaster</name>
    <probeName>cyrusmaster-${index}</probeName>
    <probeClass>jrds.probe.snmp.RdsIndexedSnmpRrd</probeClass>
    <specific name="uptimeOid">cyrusMasterInfoUptime</specific>
    <specific name="indexOid">serviceName</specific>
    <ds>
        <dsName>serviceForks</dsName>
        <dsType>counter</dsType>
        <collect optional="false">1.3.6.1.4.1.3.6.1.2.1.1</collect>
    </ds>
    <ds>
        <dsName>serviceActive</dsName>
        <dsType>gauge</dsType>
        <collect optional="false">1.3.6.1.4.1.3.6.1.2.1.2</collect>
    </ds>
    <ds>
        <dsName>serviceConnections</dsName>
        <dsType>counter</dsType>
        <collect optional="false">1.3.6.1.4.1.3.6.1.2.1.5</collect>
    </ds>
    <graphs>
        <name>CyrusMasterActive</name>
    </graphs>
</probedesc>

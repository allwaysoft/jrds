<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>mtaGroupStore</name>
    <probeName>mtagroupsnmp-${index}</probeName>
    <probeClass>jrds.probe.snmp.RdsIndexedSnmpRrd</probeClass>
    <specific name="indexOid">mtaGroupName</specific>
    <specific name="nodiscover"/>
    <ds>
        <dsName>mtaStoredMsg</dsName>
        <dsType>gauge</dsType>
        <oid>mtaGroupStoredMessages</oid>
    </ds>
    <ds>
        <dsName>mtaStoredVolume</dsName>
        <dsType>gauge</dsType>
        <oid>mtaGroupStoredVolume</oid>
    </ds>
    <graphs>
        <name>mtaStoredVolume</name>
        <name>mtaStoredMessages</name>
    </graphs>
</probedesc>

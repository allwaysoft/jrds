<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>sendmailSnmp</name>
    <probeName>sendmailsnmp</probeName>
    <probeClass>jrds.probe.snmp.RdsIndexedSnmpRrd</probeClass>
    <specific name="indexOid"/>
    <defaultargs>
        <attr delayed="false" name="index">1</attr>
    </defaultargs>
    <ds>
        <dsName>mtaReceivedMsg</dsName>
        <dsType>counter</dsType>
        <oid>mtaReceivedMessages</oid>
    </ds>
    <ds>
        <dsName>mtaStoredMsg</dsName>
        <dsType>gauge</dsType>
        <oid>mtaStoredMessages</oid>
    </ds>
    <ds>
        <dsName>mtaTransmittedMsg</dsName>
        <dsType>counter</dsType>
        <oid>mtaTransmittedMessages</oid>
    </ds>
    <ds>
        <dsName>mtaReceivedVolume</dsName>
        <dsType>counter</dsType>
        <oid>mtaReceivedVolume</oid>
    </ds>
    <ds>
        <dsName>mtaStoredVolume</dsName>
        <dsType>gauge</dsType>
        <oid>mtaStoredVolume</oid>
    </ds>
    <ds>
        <dsName>mtaTransmittedVolume</dsName>
        <dsType>counter</dsType>
        <oid>mtaTransmittedVolume</oid>
    </ds>
    <graphs>
        <name>mtaVolume</name>
        <name>mtaMessages</name>
        <name>mtaStoredVolume</name>
        <name>mtaStoredMessages</name>
    </graphs>
</probedesc>

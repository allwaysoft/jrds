<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>SunJVMG1GC7v2</name>
    <probeName>jvmpool-${label}</probeName>
    <probeClass>jrds.probe.JMX</probeClass>

    <specific name="mbeanNames">java.lang:type=GarbageCollector,name=G1 Old Generation;java.lang:type=GarbageCollector,name=G1 Young Generation;java.lang:type=MemoryPool,name=Code Cache;java.lang:type=MemoryPool,name=G1 Perm Gen</specific>
    <!-- ************ -->
    <!-- Memory pools -->
    <!-- ************ -->
    <!-- The code memory pools -->
    <ds>
        <dsName>Code Cache</dsName>
        <dsType>gauge</dsType>
        <collect>java.lang:type=MemoryPool,name=Code Cache/Usage/used</collect>
    </ds>
    <!-- The permanent memory pools -->
    <ds>
        <dsName>Perm Gen</dsName>
        <dsType>gauge</dsType>
        <collect>java.lang:type=MemoryPool,name=G1 Perm Gen/Usage/used</collect>
    </ds>
    <!-- the generational pools -->
    <ds>
        <dsName>Eden Space</dsName>
        <dsType>gauge</dsType>
        <collect>java.lang:type=MemoryPool,name=G1 Eden Space/Usage/used</collect>
    </ds>
    <ds>
        <dsName>Survivor Space</dsName>
        <dsType>gauge</dsType>
        <collect>java.lang:type=MemoryPool,name=G1 Survivor Space/Usage/used</collect>
    </ds>
    <ds>
        <dsName>Old Gen</dsName>
        <dsType>gauge</dsType>
        <collect>java.lang:type=MemoryPool,name=G1 Old Gen/Usage/used</collect>
    </ds>
    <!-- *********** -->
    <!-- GC activity -->
    <!-- *********** -->
    <!-- the major GC -->
    <ds>
        <dsName>MajorCount</dsName>
        <dsType>counter</dsType>
        <collect>java.lang:type=GarbageCollector,name=G1 Old Generation/CollectionCount</collect>
    </ds>
    <ds>
        <dsName>MajorTime</dsName>
        <dsType>counter</dsType>
        <collect>java.lang:type=GarbageCollector,name=G1 Old Generation/CollectionTime</collect>
    </ds>
    <ds>
        <dsName>MinorCount</dsName>
        <dsType>counter</dsType>
        <collect>java.lang:type=GarbageCollector,name=G1 Young Generation/CollectionCount</collect>
    </ds>
    <ds>
        <dsName>MinorTime</dsName>
        <dsType>counter</dsType>
        <collect>java.lang:type=GarbageCollector,name=G1 Young Generation/CollectionTime</collect>
    </ds>
    <!-- *********** -->
    <!-- BufferPool -->
    <!-- *********** -->
    <!-- direct -->
    <ds>
        <dsName>DirectCount</dsName>
        <dsType>gauge</dsType>
        <collect>java.nio:type=BufferPool,name=direct/Count</collect>
    </ds>
    <ds>
        <dsName>DirectUsed</dsName>
        <dsType>gauge</dsType>
        <collect>java.nio:type=BufferPool,name=direct/MemoryUsed</collect>
    </ds>
    <ds>
        <dsName>DirectCapacity</dsName>
        <dsType>gauge</dsType>
        <collect>java.nio:type=BufferPool,name=direct/TotalCapacity</collect>
    </ds>
    <!-- mapped -->
    <ds>
        <dsName>MappedCount</dsName>
        <dsType>gauge</dsType>
        <collect>java.nio:type=BufferPool,name=mapped/Count</collect>
    </ds>
    <ds>
        <dsName>MappedUsed</dsName>
        <dsType>gauge</dsType>
        <collect>java.nio:type=BufferPool,name=mapped/MemoryUsed</collect>
    </ds>
    <ds>
        <dsName>MappedCapacity</dsName>
        <dsType>gauge</dsType>
        <collect>java.nio:type=BufferPool,name=mapped/TotalCapacity</collect>
    </ds>
    <graphs>
        <name>SunJVM5MemoryPools</name>
        <name>GenGCCount</name>
        <name>GenGCSpeed</name>
        <name>GenGCTime</name>
    </graphs>
</probedesc>

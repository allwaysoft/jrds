<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>CiscoTemp</name>
    <probeName>ciscotemp-${index}</probeName>
    <probeClass>jrds.probe.snmp.RdsIndexedSnmpRrd</probeClass>
    <specific name="indexOid">ciscoEnvMonTemperatureStatusDescr</specific>
    <ds>
        <dsName>temp</dsName>
        <dsType>gauge</dsType>
        <oid>ciscoEnvMonTemperatureStatusValue</oid>
    </ds>
    <ds>
        <dsName>threshold</dsName>
        <dsType>gauge</dsType>
        <oid>ciscoEnvMonTemperatureThreshold</oid>
    </ds>
    <graphs>
        <name>TempThresold</name>
    </graphs>
</probedesc>

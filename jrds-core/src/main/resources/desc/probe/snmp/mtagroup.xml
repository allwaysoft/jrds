<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>mtaGroup</name>
    <probeName>mtagroupsnmp-${index}</probeName>
    <probeClass>jrds.probe.snmp.RdsIndexedSnmpRrd</probeClass>
    <specific name="indexOid">mtaGroupName</specific>
    <ds>
        <dsName>mtaReceivedMsg</dsName>
        <dsType>counter</dsType>
        <oid>mtaGroupReceivedMessages</oid>
    </ds>
    <ds>
        <dsName>mtaGroupRejectedMsg</dsName>
        <dsType>counter</dsType>
        <oid>mtaGroupRejectedMessages</oid>
    </ds>
    <ds>
        <dsName>mtaStoredMsg</dsName>
        <dsType>gauge</dsType>
        <oid>mtaGroupStoredMessages</oid>
    </ds>
    <ds>
        <dsName>mtaTransmittedMsg</dsName>
        <dsType>counter</dsType>
        <oid>mtaGroupTransmittedMessages</oid>
    </ds>
    <ds>
        <dsName>mtaReceivedVolume</dsName>
        <dsType>counter</dsType>
        <oid>mtaGroupReceivedVolume</oid>
    </ds>
    <ds>
        <dsName>mtaStoredVolume</dsName>
        <dsType>gauge</dsType>
        <oid>mtaGroupStoredVolume</oid>
    </ds>
    <ds>
        <dsName>mtaTransmittedVolume</dsName>
        <dsType>counter</dsType>
        <oid>mtaGroupTransmittedVolume</oid>
    </ds>
    <graphs>
        <name>mtaVolume</name>
        <name>mtaMessages</name>
        <name>mtaStoredVolume</name>
        <name>mtaStoredMessages</name>
    </graphs>
</probedesc>

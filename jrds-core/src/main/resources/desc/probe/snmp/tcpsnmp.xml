<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>TcpSnmp</name>
    <probeName>tcp_snmp</probeName>
    <probeClass>jrds.probe.snmp.RdsSnmpSimple</probeClass>
    <specific name="existOid">tcpCurrEstab</specific>
    <specific name="requester">simple</specific>
    <ds>
        <dsName>ActiveOpens</dsName>
        <dsType>counter</dsType>
        <oid>tcpActiveOpens</oid>
    </ds>
    <ds>
        <dsName>PassiveOpens</dsName>
        <dsType>counter</dsType>
        <oid>tcpPassiveOpens</oid>
    </ds>
    <ds>
        <dsName>AttemptFails</dsName>
        <dsType>counter</dsType>
        <oid>tcpAttemptFails</oid>
    </ds>
    <ds>
        <dsName>EstabResets</dsName>
        <dsType>counter</dsType>
        <oid>tcpEstabResets</oid>
    </ds>
    <ds>
        <dsName>CurrEstab</dsName>
        <dsType>gauge</dsType>
        <oid>tcpCurrEstab</oid>
    </ds>
    <ds>
        <dsName>InSegs</dsName>
        <dsType>counter</dsType>
        <oid>tcpInSegs</oid>
    </ds>
    <ds>
        <dsName>OutSegs</dsName>
        <dsType>counter</dsType>
        <oid>tcpOutSegs</oid>
    </ds>
    <ds>
        <dsName>RetransSegs</dsName>
        <dsType>counter</dsType>
        <oid>tcpRetransSegs</oid>
    </ds>
    <ds>
        <dsName>InErrs</dsName>
        <dsType>counter</dsType>
        <oid>tcpInErrs</oid>
    </ds>
    <ds>
        <dsName>OutRsts</dsName>
        <dsType>counter</dsType>
        <oid>tcpOutRsts</oid>
    </ds>
    <graphs>
        <name>tcpconnection</name>
        <name>tcpestablished</name>
        <name>tcpsegments</name>
    </graphs>
</probedesc>
